<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <GridView
        android:layout_width="match_parent"
        android:id="@+id/action"
        android:layout_height="100dp"
        android:numColumns="3"
        android:background="#AAA"/>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <FrameLayout
            android:id="@+id/container"
            android:layout_width="match_parent"
            android:layout_height="480dp"
            android:background="#000"
            tools:context="org.tensorflow.demo.CameraActivity">
        </FrameLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_gravity="center"
            android:padding="30dp"
            >

            <GridLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:columnCount="3"
                android:layout_gravity="center">

                <GridLayout
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:columnCount="2"
                    android:layout_gravity="center">
                </GridLayout>

                <GridLayout
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:columnCount="2"
                    android:layout_gravity="center">
                    <TextView
                        android:id="@+id/up1"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_margin="1dp"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:text="" />
                    <TextView
                        android:id="@+id/up2"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_margin="1dp"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:text="" />
                    <TextView
                        android:id="@+id/up3"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_margin="1dp"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:text="" />
                    <TextView
                        android:id="@+id/up4"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_margin="1dp"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:text="" />
                </GridLayout>

                <GridLayout
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:columnCount="2"
                    android:layout_gravity="center">
                </GridLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/up"
                    android:layout_margin="1dp"
                    android:layout_gravity="center"
                    android:textColor="#FFFFFF"
                    android:text="Up"
                    android:textSize="25sp"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"/>

                <GridLayout
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:columnCount="2"
                    android:layout_gravity="center">
                    <TextView
                        android:id="@+id/left1"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_margin="1dp"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:text="" />
                    <TextView
                        android:id="@+id/left2"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_margin="1dp"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:text="" />
                    <TextView
                        android:id="@+id/left3"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_margin="1dp"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:text="" />
                    <TextView
                        android:id="@+id/left4"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_margin="1dp"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:text="" />
                </GridLayout>

                <GridLayout
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:columnCount="2"
                    android:layout_gravity="center"
                    android:padding="3dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:background="#9999CC">
                    <TextView
                        android:id="@+id/front1"
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:layout_margin="1dp"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:text="" />
                    <TextView
                        android:id="@+id/front2"
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:layout_margin="1dp"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:text="" />
                    <TextView
                        android:id="@+id/front3"
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:layout_margin="1dp"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:text="" />
                    <TextView
                        android:id="@+id/front4"
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:layout_margin="1dp"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:text="" />
                </GridLayout>

                <GridLayout
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:columnCount="2"
                    android:layout_gravity="center">
                    <TextView
                        android:id="@+id/right1"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_margin="1dp"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:text="" />
                    <TextView
                        android:id="@+id/right2"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_margin="1dp"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:text="" />
                    <TextView
                        android:id="@+id/right3"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_margin="1dp"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:text="" />
                    <TextView
                        android:id="@+id/right4"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_margin="1dp"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:text="" />
                </GridLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/left"
                    android:layout_margin="1dp"
                    android:layout_gravity="center"
                    android:textColor="#FFFFFF"
                    android:text="Left"
                    android:textSize="25sp"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/front"
                    android:layout_margin="1dp"
                    android:layout_gravity="center"
                    android:textColor="#FFFFFF"
                    android:text="Front"
                    android:textSize="25sp"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/right"
                    android:layout_margin="1dp"
                    android:layout_gravity="center"
                    android:textColor="#FFFFFF"
                    android:text="Right"
                    android:textSize="25sp"/>

                <GridLayout
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:columnCount="2"
                    android:layout_gravity="center">
                </GridLayout>

                <GridLayout
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:columnCount="2"
                    android:layout_gravity="center">
                    <TextView
                        android:id="@+id/down1"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_margin="1dp"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:text="" />
                    <TextView
                        android:id="@+id/down2"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_margin="1dp"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:text="" />
                    <TextView
                        android:id="@+id/down3"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_margin="1dp"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:text="" />
                    <TextView
                        android:id="@+id/down4"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_margin="1dp"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:text="" />
                </GridLayout>

                <GridLayout
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:columnCount="2"
                    android:layout_gravity="center">

                    <TextView
                        android:id="@+id/back2"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_margin="1dp"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:text="" />
                    <TextView
                        android:id="@+id/back1"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_margin="1dp"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:text="" />

                    <TextView
                        android:id="@+id/back4"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_margin="1dp"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:text="" />
                    <TextView
                        android:id="@+id/back3"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_margin="1dp"
                        android:layout_gravity="center"
                        android:background="#FFFFFF"
                        android:text="" />

                </GridLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/down"
                    android:layout_margin="1dp"
                    android:layout_gravity="center"
                    android:textColor="#FFFFFF"
                    android:text="Down"
                    android:textSize="25sp"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/back"
                    android:layout_margin="1dp"
                    android:layout_gravity="center"
                    android:textColor="#FFFFFF"
                    android:text="  Back\n Mirror"
                    android:textSize="25sp"/>

            </GridLayout>



        </LinearLayout>







    </FrameLayout>









</LinearLayout>


